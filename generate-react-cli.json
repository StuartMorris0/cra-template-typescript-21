{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "styl",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/components/TemplateName.tsx",
        "style": "templates/components/TemplateName.styles.ts",
        "test": "templates/components/TemplateName.test.tsx",
        "index": "templates/components/index.ts"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false,
      "withIndex": true
    },
    "page": {
      "customTemplates": {
        "component": "templates/pages/TemplateName.tsx",
        "test": "templates/pages/TemplateName.test.tsx"
      },
      "path": "src/pages",
      "withTest": true,
      "withStory": false,
      "withLazy": false
    }
  }
}
